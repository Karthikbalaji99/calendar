{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "functions": {
  "dist/index.js": {
    "runtime": "nodejs22.x",
    "maxDuration": 10
  }
},
  "routes": [
    { "src": "^/api(/.*)?$", "dest": "/api/index.js" },
    { "src": "^/favicon\\.ico$", "dest": "/favicon.png" },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
